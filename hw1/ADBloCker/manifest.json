      "rule_resources": [{ 
        "id": "ruleset_1", 
        "enabled": true, 
        "path": "rules.json" 
      }] 
    }, 
   
    "permissions": [ 
      "declarativeNetRequest" 
    ], 

    "background": {
        "service_worker": "background.js"
    }, 
   
    "manifest_version": 3 
  }  
  